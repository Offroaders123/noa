{
  "name": "noa-engine",
  "version": "0.22.0",
  "description": "Experimental voxel game engine",
  "main": "src/index.js",
  "scripts": {
    "start": "sleep 1 && open http://localhost:8080/ & (cd docs/hello-world/; webpack-dev-server -d --inline index.js) || true",
    "test": "sleep 1 && open http://localhost:8080/ & (cd docs/test/; webpack-dev-server -d --inline index.js) || true",
    "doc": "cat tools/buildDocs.js | node"
  },
  "author": "Andy Hall",
  "keywords": [
    "voxel",
    "game",
    "engine"
  ],
  "license": "MIT",
  "dependencies": {
    "aabb-3d": "github:fenomas/aabb-3d#c5698961161884397c711e8fb533611642a8a487",
    "box-intersect": "^1.0.1",
    "ent-comp": "^0.5.0",
    "extend": "^3.0.0",
    "fast-voxel-raycast": "^0.1.1",
    "game-inputs": "^0.2.0",
    "game-shell": "andyhall/game-shell",
    "gl-vec3": "^1.0.3",
    "ndarray": "^1.0.16",
    "ndarray-hash": "^1.0.0",
    "voxel-aabb-sweep": "^0.5.0",
    "voxel-physics-engine": "^0.7.3"
  },
  "repository": "https://github.com/andyhall/noa",
  "bugs": {
    "url": "https://github.com/andyhall/noa/issues"
  },
  "devDependencies": {
    "markdox": "^0.1.10"
  },
  "overrides": {
    "aabb-3d": "$aabb-3d"
  }
}
